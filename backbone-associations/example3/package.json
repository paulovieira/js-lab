{
  "dependencies": {
    "browserify": "^9.0.3",
    "browserify-shim": "^3.8.3",
    "watchify": "^2.5.0"
  },
  "browserify-shim": {
    "jQuery": "global:jQuery",
    "Underscore": "global:_",
    "Backbone": "global:Backbone",
